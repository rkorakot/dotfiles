#GR='\[\e[32m\]' # Green
#WH='\[\e[37m\]' # White
#YE='\[\e[33m\]' # Yellow
#RE='\[\e[31m\]' # Red
#NC='\[\e[0m\]'  # Reset
GR='\033[32m' # Green
WH='\033[37m' # White
YE='\033[33m' # Yellow
RE='\033[31m' # Red
NC='\033[0m'  # Reset
export PS1="${GR}\u${NC} ${WH}:${NC} ${YE}\w${NC}\n${RE}\$ ${NC}"
export LC_ALL=en_US.UTF-8
